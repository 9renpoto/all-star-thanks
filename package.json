{
  "name": "all-star-thanks",
  "version": "0.0.1",
  "author": "Keisuke Kan <9renpoto@gmail.com>",
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "4.4.0",
    "@9renpoto/eslint-config-react": "4.3.0",
    "@9renpoto/tslint-config": "4.0.0",
    "@storybook/react": "3.4.5",
    "@types/faker": "4.1.2",
    "@types/storybook__react": "3.0.7",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "cpx": "1.5.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "faker": "4.1.0",
    "flow-bin": "0.72.0",
    "flow-typed": "2.4.0",
    "gh-pages": "1.1.0",
    "husky": "0.14.3",
    "lint-staged": "7.1.2",
    "npm-run-all": "4.1.3",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "prettier-eslint-cli": "4.7.1",
    "style-loader": "0.21.0",
    "tslint": "5.10.0",
    "tslint-react": "3.6.0",
    "typescript": "2.8.3",
    "url-loader": "1.0.1"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.css": [
      "prettier --parser postcss --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/9renpoto/all-star-thanks.git",
  "scripts": {
    "build": "run-s clean build:*",
    "build:assets": "cpx 'src/**/*.{css,jpeg}' lib",
    "build:js": "babel src -d lib",
    "clean": "rimraf coverage .stoybook/build lib",
    "deploy": "run-s clean storybook",
    "lint": "tslint {src,stories,__tests__}/**/*.ts{,x}",
    "precommit": "lint-staged",
    "start": "start-storybook -p 9001",
    "storybook": "build-storybook -o ./.storybook/build && gh-pages -d ./.storybook/build"
  },
  "style": "lib/bundle.css",
  "types": "lib/index.d.ts"
}
