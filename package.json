{
  "name": "all-star-thanks",
  "version": "0.0.1",
  "author": "Keisuke Kan <9renpoto@gmail.com>",
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "^4.1.0",
    "@9renpoto/eslint-config-react": "^4.0.0",
    "@9renpoto/tslint-config": "^4.0.0",
    "@storybook/react": "^3.3.13",
    "@types/faker": "^4.1.2",
    "@types/storybook__react": "^3.0.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.65.0",
    "flow-typed": "^2.3.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "npm-run-all": "^4.1.2",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prettier-eslint-cli": "^4.7.1",
    "style-loader": "^0.20.2",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "url-loader": "^0.6.2"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.css": [
      "prettier --parser postcss --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/9renpoto/all-star-thanks.git",
  "scripts": {
    "build": "run-s clean build:*",
    "build:assets": "cpx 'src/**/*.{css,jpeg}' lib",
    "build:js": "babel src -d lib",
    "clean": "rimraf coverage .stoybook/build lib",
    "deploy": "run-s clean storybook",
    "lint": "tslint {src,stories,__tests__}/**/*.ts{,x}",
    "precommit": "lint-staged",
    "start": "start-storybook -p 9001",
    "storybook": "build-storybook -o ./.storybook/build && gh-pages -d ./.storybook/build"
  },
  "style": "lib/bundle.css",
  "types": "lib/index.d.ts"
}
