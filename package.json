{
  "name": "all-star-thanks",
  "version": "0.0.1",
  "author": "Keisuke Kan <9renpoto@gmail.com>",
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "4.7.0",
    "@9renpoto/eslint-config-react": "4.7.0",
    "@9renpoto/tslint-config": "4.7.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "4.0.12",
    "@types/faker": "4.1.4",
    "@types/storybook__react": "4.0.0",
    "babel-loader": "8.0.4",
    "cpx": "1.5.0",
    "css-loader": "2.0.0",
    "eslint": "5.10.0",
    "faker": "4.1.0",
    "flow-bin": "0.88.0",
    "flow-typed": "2.5.1",
    "gh-pages": "2.0.1",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-test-renderer": "^16.6.3",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.{css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/9renpoto/all-star-thanks.git",
  "scripts": {
    "build": "run-s clean build:*",
    "build:assets": "cpx 'src/**/*.{css,jpeg}' lib",
    "build:js": "babel src -d lib",
    "clean": "rimraf coverage .stoybook/build lib",
    "deploy": "run-s clean storybook",
    "lint": "tslint {src,stories,__tests__}/**/*.ts{,x}",
    "start": "start-storybook -p 9001",
    "storybook": "build-storybook -o ./.storybook/build && gh-pages -d ./.storybook/build"
  },
  "style": "lib/bundle.css",
  "types": "lib/index.d.ts"
}
