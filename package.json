{
  "name": "all-star-thanks",
  "description": "like `all-star-thanks` animation components",
  "version": "0.0.1",
  "author": "Keisuke Kan <9renpoto@gmail.com>",
  "bugs": {
    "url": "https://github.com/9renpoto/all-star-thanks/issues"
  },
  "devDependencies": {
    "@9renpoto/eslint-config-flowtype": "5.4.0",
    "@9renpoto/eslint-config-react": "5.4.0",
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-flow": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "@storybook/addon-storyshots": "5.2.8",
    "@storybook/react": "5.2.8",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "codecov": "3.6.1",
    "cpx": "1.5.0",
    "css-modules-flow-types-cli": "1.3.0",
    "eslint": "6.7.2",
    "faker": "4.1.0",
    "flow-bin": "0.112.0",
    "gh-pages": "2.1.1",
    "husky": "3.1.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-date-mock": "1.0.7",
    "lint-staged": "9.5.0",
    "mockdate": "2.0.5",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0"
  },
  "homepage": "https://9renpoto.github.io/all-star-thanks",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/stories/",
      "/.storybook/",
      "<rootDir>/node_modules/",
      "story(.*).tsx",
      "<rootDir>/setup.js"
    ],
    "setupFiles": [
      "<rootDir>/setup.js",
      "jest-date-mock"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "keywords": [
    "react",
    "styleguide"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write",
      "eslint --cache",
      "git add"
    ],
    "*.{css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "repository": "ssh://git@github.com/9renpoto/all-star-thanks.git",
  "scripts": {
    "build": "run-s clean build:*",
    "build:assets": "cpx 'src/**/*.{css,jpeg}' lib",
    "build:js": "babel src -d lib",
    "clean": "rimraf coverage .stoybook/build lib",
    "coverage": "codecov -f ./coverage/lcov.info",
    "deploy": "run-s clean storybook",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint . --ext=js,jsx",
    "lint:flow": "css-modules-flow-types src && flow",
    "start": "start-storybook -p 9001",
    "storybook": "build-storybook -o ./.storybook/build && gh-pages -d ./.storybook/build",
    "test": "jest"
  },
  "style": "lib/bundle.css",
  "types": "lib/index.d.ts"
}
